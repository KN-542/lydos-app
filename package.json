{
  "name": "lydos-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --port 8090",
    "android": "expo start --android --port 8090",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "lint": "biome check --write",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "generate:api": "curl -k https://local.api.lydos/doc -o src/lib/openapi.json && openapi-typescript src/lib/openapi.json -o src/lib/api-types.ts"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.19.23",
    "@tanstack/react-query": "^5.90.21",
    "expo": "~54.0.33",
    "expo-auth-session": "~7.0.10",
    "expo-constants": "~18.0.13",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-secure-store": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "lucide-react-native": "^0.564.0",
    "openapi-fetch": "^0.17.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.15.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/react": "~19.1.0",
    "openapi-typescript": "^7.13.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
